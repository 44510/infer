{
  "force-delete-results-dir": true,
  "annotation-reachability-expensive": true,
  "annotation-reachability-no-allocation": true,
  "modeled-expensive": [
    {
      "class": "android.app.Activity",
      "method": "findViewById"
    },
    {
      "class": "android.view.View",
      "method": "findViewById"
    }
  ],
  "annotation-reachability-custom-pairs": [
    {
      "sources": ["UserDefinedSource1", "UserDefinedSource2"],
      "sinks": ["UserDefinedSink1", "UserDefinedSink2"],
      "sanitizers": ["UserDefinedSanitizer"]
    },
    {
      "sources": ["AnyThread", "WorkerThread"],
      "sinks": ["UiThread", "MainThread"]
    },
    {
      "sources": ["AnyThread", "MainThread", "UiThread"],
      "sinks": ["WorkerThread"]
    }
  ],
  "annotation-reachability-custom-models":
  [
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sourceDefinedInConfigOk",
      "annotation": "UserDefinedSource1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sinkDefinedInConfig",
      "annotation": "UserDefinedSink1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sourceDefinedInConfigBad",
      "annotation": "UserDefinedSource1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sanitizerDefinedInConfig",
      "annotation": "UserDefinedSanitizer"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\$Base\\.sourceDefinedInConfigBad",
      "annotation": "UserDefinedSource1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\$Base\\.sinkDefinedInConfig",
      "annotation": "UserDefinedSink1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sourceDefinedInConfig.*WithRegex.*",
      "annotation": "UserDefinedSource1"
    },
    {
      "method_regex": "codetoanalyze\\.java\\.checkers\\.CustomAnnotations\\.sinkDefinedInConfig.*WithRegex",
      "annotation": "UserDefinedSink1"
    }
  ]
}
