{
  "force-delete-results-dir": true,
  "annotation-reachability-expensive": true,
  "annotation-reachability-no-allocation": true,
  "modeled-expensive": [
    {
      "class": "android.app.Activity",
      "method": "findViewById"
    },
    {
      "class": "android.view.View",
      "method": "findViewById"
    }
  ],
  "annotation-reachability-custom-pairs": [
    {
      "sources": ["UserDefinedSource1", "UserDefinedSource2"],
      "sinks": ["UserDefinedSink1", "UserDefinedSink2"],
      "sanitizers": ["UserDefinedSanitizer"]
    },
    {
      "sources": ["AnyThread", "WorkerThread"],
      "sinks": ["UiThread", "MainThread"]
    },
    {
      "sources": ["AnyThread", "MainThread", "UiThread"],
      "sinks": ["WorkerThread"]
    }
  ],
  "annotation-reachability-custom-models":
  [
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sourceDefinedInConfigOk",
      "annotation": "UserDefinedSource1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sinkDefinedInConfig",
      "annotation": "UserDefinedSink1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sourceDefinedInConfigBad",
      "annotation": "UserDefinedSource1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sanitizerDefinedInConfig",
      "annotation": "UserDefinedSanitizer"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations$Base",
      "method_regex": "sourceDefinedInConfigBad",
      "annotation": "UserDefinedSource1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations$Base",
      "method_regex": "sinkDefinedInConfig",
      "annotation": "UserDefinedSink1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sourceDefinedInConfig.*WithRegex.*",
      "annotation": "UserDefinedSource1"
    },
    {
      "class_name": "codetoanalyze.java.checkers.CustomAnnotations",
      "method_regex": "sinkDefinedInConfig.*WithRegex",
      "annotation": "UserDefinedSink1"
    }
  ]
}
